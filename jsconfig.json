{
  "compilerOptions": {
    "baseUrl": "/",
    "paths": {
      "@/*": ["src/*"],
      "@Image/*": ["src/assets/*"],
      "@Components/*": ["src/components/*"],
      "@Imports/*": ["src/imports/*"],
      "@Pages/*": ["src/pages/*"],
      "@Styles/*": ["src/styles/*"],
      "@useEffect/*": ["src/component/useEffect/*"],
      "@useState/*": ["src/component/useState/*"]
    }
  }
}
